{
  "name": "@mintlify/previewing",
  "version": "4.0.14",
  "description": "Preview Mintlify docs locally",
  "engines": {
    "node": ">=18.0.0"
  },
  "author": "Mintlify, Inc.",
  "bugs": {
    "url": "https://github.com/mintlify/docs/issues"
  },
  "license": "Elastic-2.0",
  "keywords": [
    "mintlify",
    "mint",
    "previewing"
  ],
  "type": "module",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepare": "npm run build",
    "build": "tsc",
    "clean:build": "rimraf dist",
    "clean:all": "rimraf node_modules .eslintcache && yarn clean:build",
    "watch": "tsc --watch",
    "format": "prettier \"./src/**/*.ts\" --write",
    "format:check": "prettier \"./src/**/*.ts\" --check",
    "lint": "eslint . --cache",
    "type": "tsc --noEmit"
  },
  "peerDependencies": {
    "@mintlify/validation": ">= 0.1.48 < 1"
  },
  "dependencies": {
    "@apidevtools/swagger-parser": "^10.1.0",
    "@mintlify/prebuild": "1.0.42",
    "@octokit/rest": "^19.0.5",
    "chalk": "^5.1.0",
    "chokidar": "^3.5.3",
    "express": "^4.18.2",
    "fs-extra": "^11.1.0",
    "got": "^13.0.0",
    "gray-matter": "^4.0.3",
    "is-absolute-url": "^4.0.1",
    "is-online": "^10.0.0",
    "open": "^8.4.0",
    "openapi-types": "^12.0.2",
    "ora": "^6.1.2",
    "remark": "^14.0.2",
    "remark-frontmatter": "^4.0.1",
    "remark-gfm": "^3.0.1",
    "remark-math": "^5.1.1",
    "remark-mdx": "^2.2.1",
    "socket.io": "^4.7.2",
    "tar": "^6.1.15",
    "unist-util-visit": "^4.1.1",
    "yargs": "^17.6.0"
  },
  "devDependencies": {
    "@mintlify/eslint-config": "1.0.3",
    "@mintlify/eslint-config-typescript": "1.0.7",
    "@mintlify/prettier-config": "1.0.1",
    "@mintlify/ts-config": "2.0.0",
    "@mintlify/validation": "0.1.71",
    "@trivago/prettier-plugin-sort-imports": "3.x",
    "@tsconfig/recommended": "1.x",
    "@types/fs-extra": "^9.0.13",
    "@types/got": "^9.6.12",
    "@types/inquirer": "^9.0.1",
    "@types/node": "^18.7.13",
    "@types/tar": "^6.1.5",
    "@types/yargs": "^17.0.13",
    "@typescript-eslint/eslint-plugin": "5.x",
    "@typescript-eslint/parser": "5.x",
    "eslint": "8.x",
    "eslint-config-prettier": "8.x",
    "eslint-plugin-unused-imports": "2.x",
    "prettier": "2.x",
    "rimraf": "^5.0.1",
    "typescript": "^4.8.2"
  },
  "gitHead": "53cadb2b40e4a0edb392e42d2715d9fd08117218"
}
